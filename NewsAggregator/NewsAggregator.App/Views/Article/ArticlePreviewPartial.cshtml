@model ArticleViewModel

<div class="col-12">
    <div class="card">
        <div class="card-body">
            <h2 class="card-title">@Model.Title</h2>
            <h5 class="card-text">@Model.Description</h5>
            <p class="card-text">@Model.Body</p>
        </div>
        <div class="card-footer text-muted">
            Published: @Model.CreationDate
        </div>
    </div>
</div>
