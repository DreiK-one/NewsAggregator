@model UserViewModel

@{
	ViewData["Title"] = "User info";
}

<div id="gtco-main">
	<div class="container">
		<div class="row row-pb-md">
			<div class="col-md-12">
				<h1>User @Model.FullName</h1>
				<ul>
					<li>User first name: @Model.FirstName</li>
					<li>User last name: @Model.LastName</li>
					<li>User email: @Model.Email</li>
					<li>Date of registration: @Model.RegistrationDate</li>
				</ul>
			</div>
		</div>
	</div>
</div>
